	$(srcdir)/t_nfold.c	\
	$(srcdir)/t_encrypt.c	\
	$(srcdir)/t_decrypt.c	\
	$(srcdir)/t_prf.c 	\
	$(srcdir)/t_prng.c	\
	$(srcdir)/t_cmac.c	\
	$(srcdir)/t_hmac.c	\
	$(srcdir)/t_pkcs5.c	\
	$(srcdir)/t_cts.c	\
	$(srcdir)/vectors.c	\
	$(srcdir)/aes-test.c	\
	$(srcdir)/camellia-test.c	\
	$(srcdir)/t_cksum.c	\
	$(srcdir)/t_cksums.c	\
	$(srcdir)/t_crc.c	\
	$(srcdir)/t_mddriver.c	\
	$(srcdir)/t_kperf.c	\
	$(srcdir)/t_short.c	\
